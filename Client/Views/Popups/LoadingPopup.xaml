<?xml version="1.0" encoding="utf-8" ?>
<mopups:PopupPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Client.Views.Popups.LoadingPopup"
             Title="LoadingPopup"
             xmlns:mopups="clr-namespace:Mopups.Pages;assembly=Mopups"
              Background="#8000"
             CloseWhenBackgroundIsClicked="False">
    <Frame HeightRequest="50" CornerRadius="5" WidthRequest="50" BackgroundColor="White">
        <ActivityIndicator Color="Orange"  IsRunning="True" HeightRequest="40" WidthRequest="40" />
    </Frame>
</mopups:PopupPage>